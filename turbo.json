{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "dev": {
      "cache": false,
      "dependsOn": ["^build"],
      "persistent": true
    },
    "build": {
      "outputs": ["dist/**"]
    },
    "lint": {
      "cache": false
    },
    "tsc": {
      "cache": false
    }
  }
}
